KISSY.add("malldetail/other/limitedPurchase",function(e,t,a){var i=function(e){var i=e.el;var n=e.product;n.onLoad(["itemDO","config"],function(e,n){var o="";var r;if(n.isOnlyInMobile){r=t.get(".tb-action",i);o=TShop.getQrURL({itemId:e.itemId,mmstat:"detail_pcsku"});if(r){t.insertBefore(t.create(a({src:o})),r)}else{t.append(t.create(a({src:o})),i)}}})};return{init:i}},{requires:["dom","malldetail/other/limitedPurchase.tpl","malldetail/other/limitedPurchase.css"]});